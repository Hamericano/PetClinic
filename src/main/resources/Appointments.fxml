<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.Controller">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" styleClass="bodybgappointments" stylesheets="@background.css" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="183.0" layoutY="30.0" text="APPOINTMENTS" textAlignment="CENTER" textFill="WHITE" underline="true">
               <font>
                  <Font name="System Bold" size="35.0" />
               </font>
            </Label>
            <Button layoutX="260.0" layoutY="106.0" mnemonicParsing="false" onAction="#addAppointment" prefHeight="31.0" prefWidth="100.0" text="Add" />
            <Button layoutX="260.0" layoutY="144.0" mnemonicParsing="false" onAction="#viewAppointments" prefHeight="31.0" prefWidth="100.0" text="View" />
            <Button layoutX="260.0" layoutY="185.0" mnemonicParsing="false" onAction="#updateAppointment" prefHeight="31.0" prefWidth="100.0" text="Update" />
            <Button layoutX="260.0" layoutY="228.0" mnemonicParsing="false" onAction="#deleteAppointment" prefHeight="31.0" prefWidth="100.0" text="Delete" />
            <Button layoutX="260.0" layoutY="302.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="31.0" prefWidth="100.0" text="Back" />
         </children>
    </AnchorPane>
  </children>
</VBox>
